{
  "name": "griddle-overhaul-redux",
  "version": "0.0.30",
  "description": "Injects consumers redux with griddle",
  "main": "lib/index.js",
  "scripts": {
    "prebuild": "rimraf build",
    "build": "babel src/lib -d lib",
    "watch": "npm run build -- --watch",
    "predoc": "rimraf doc",
    "doc": "esdoc -c ./esdoc.json",
    "prerelease": "npm run build",
    "release": "npm version patch && npm publish",
    "postrelease": "npm run release-doc",
    "prerelease-doc": "npm run doc",
    "release-doc": "git subtree push --prefix doc origin gh-pages",
    "postrelease-doc": "git commit -am \"doc-release\" && git push --follow-tags",
    "start": "./node_modules/.bin/webpack --config webpack.config.js --watch",
    "test": "./node_modules/.bin/karma start --browsers PhantomJS",
    "test-debug": "./node_modules/.bin/karma start --browsers Chrome"
  },
  "keywords": [
    "griddle",
    "react",
    "component",
    "higher-order"
  ],
  "author": "Griddle",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^2.7.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-react": "^4.3.0",
    "rimraf": "^2.5.2",
    "griddle-overhaul-core": "*",
    "jasmine-core": "^2.2.0",
    "jsx-loader": "~0.12.2",
    "karma": "^0.13.*",
    "karma-babel-preprocessor": "^5.0.1",
    "karma-chrome-launcher": "^0.1.8",
    "karma-cli": "0.0.4",
    "karma-jasmine": "^0.3.5",
    "karma-phantomjs-launcher": "^0.1.4",
    "karma-webpack": "^1.7.0",
    "phantomjs": "^1.9.18",
    "phantomjs-polyfill": "0.0.1",
    "webpack": "*",
    "webpack-dev-server": "*",
    "react-redux": "*",
    "redux": "*",
    "redux-thunk": "*"
  },
  "dependencies": {
    "esdoc-es7-plugin": "0.0.3",
    "lodash.compose": "^2.4.1",
    "object-assign": "~2.0.0"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0"
  },
  "bugs": {
    "url": "https://github.com/cchamberlain/griddle-overhaul-redux/issues"
  },
  "homepage": "https://github.com/cchamberlain/griddle-overhaul-redux#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cchamberlain/griddle-overhaul-redux.git"
  }
}
